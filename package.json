{
  "name": "sanity-template-ecommerce-studio",
  "description": "Sanity + Shopify: A Powerful Ecommerce Content Management System",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "sanity dev --dir=studio",
    "dev:studio": "sanity dev --dir=studio",
    "build": "sanity build --dir=studio",
    "deploy": "sanity deploy --dir=studio",
    "deploy-graphql": "sanity graphql deploy --dir=studio",
    "schema:deploy": "sanity schema deploy --dir=studio",
    "typegen": "sanity typegen generate --dir=studio",
    "validate": "sanity-template-validate"
  },
  "keywords": [
    "sanity",
    "shopify",
    "ecommerce",
    "headless-cms",
    "content-management"
  ],
  "workspaces": [
    "studio"
  ],
  "devDependencies": {
    "@sanity/template-validator": "^2.4.3",
    "npm-run-all2": "^5.0.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "dependencies": {
    "react-is": "^19.1.0",
    "styled-components": "^6.1.19"
  }
}
